{
  "name": "artillery-plugin-memory-inspector",
  "version": "1.1.1",
  "description": "Inspect the memory of processes running together with your load tests!",
  "main": "index.js",
  "scripts": {
    "test": "tap ./test/*.spec.mjs --timeout 300"
  },
  "tap": {
    "disable-coverage": true,
    "allow-empty-coverage": true,
    "color": true,
    "test-env": [
      "ARTILLERY_TELEMETRY_DEFAULTS={\"source\":\"test-suite\"}"
    ]
  },
  "author": "",
  "license": "MPL-2.0",
  "dependencies": {
    "debug": "^4.3.4",
    "pidusage": "^3.0.2"
  },
  "devDependencies": {
    "portfinder": "^1.0.32",
    "tap": "^19.0.2",
    "zx": "^7.2.3"
  }
}
