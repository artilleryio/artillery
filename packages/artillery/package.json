{
  "name": "artillery",
  "version": "2.0.0-27",
  "description": "Modern performance testing toolkit for DevOps and SRE",
  "main": "./lib",
  "engines": {
    "node": ">= 16.15.0"
  },
  "files": [
    "/bin",
    "/lib",
    "console-reporter.js",
    "util.js",
    ".artilleryrc"
  ],
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "useTabs": false,
    "trailingComma": "none"
  },
  "oclif": {
    "update": {
      "s3": {
        "bucket": "artillery-cli-assets"
      }
    },
    "commands": "./lib/cmds",
    "hooks": {
      "init": [
        "./lib/cli/hooks/version"
      ]
    },
    "bin": "artillery",
    "_helpClass": "./bin/help",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ],
    "topics": {
      "aws": {
        "description": "run tests on AWS"
      },
      "pro": {
        "description": "deploy and manage Artillery Pro"
      }
    }
  },
  "scripts": {
    "test": "export ARTILLERY_TELEMETRY_DEFAULTS='{\"source\":\"test-suite\"}' && tap --no-coverage --timeout=300 test/unit/*.test.js test/cli/probe/*.js test/cli/*.test.js && bash test/runner.sh && bash test/lib/run.sh && node test/testcases/plugins/*.test.js",
    "is_linted": "find . -name '*.js' | grep -v node_modules | xargs eslint",
    "lint": "eslint --ext \".js,.ts,.tsx\" --ignore-path .gitignore .",
    "lint-fix": "npm run lint -- --fix",
    "commitlint-last-commit": "commitlint --from $(git show HEAD~1 | grep commit | awk '{print $2}')"
  },
  "tap": {
    "check-coverage": false
  },
  "keywords": [
    "load testing",
    "stress testing",
    "benchmark",
    "performance",
    "blackbox testing"
  ],
  "author": "Hassy Veldstra <h@veldstra.org>",
  "contributors": [
    "Kieran Gorman (https://github.com/kjgorman)",
    "Antony Jones (https://github.com/antony)",
    "Joe Schofield (https://github.com/JoeScho)",
    "Kush Jain (https://github.com/kush-jain)"
  ],
  "license": "MPL-2.0",
  "preferGlobal": true,
  "man": "./man/artillery.1",
  "bin": {
    "artillery": "./bin/run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/artilleryio/artillery.git"
  },
  "bugs": {
    "url": "https://github.com/artilleryio/artillery/issues",
    "email": "npm@veldstra.org"
  },
  "dependencies": {
    "@aws-sdk/credential-providers": "^3.127.0",
    "@oclif/command": "^1.8.16",
    "@oclif/config": "^1.18.3",
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/plugin-help": "^3.3.1",
    "@oclif/plugin-not-found": "^2.3.1",
    "archiver": "^5.3.1",
    "artillery-plugin-ensure": "^1.1.1",
    "artillery-plugin-expect": "^2.1.0",
    "artillery-plugin-metrics-by-endpoint": "^1.0.2",
    "artillery-plugin-publish-metrics": "^2.0.2",
    "async": "^2.6.4",
    "aws-sdk": "^2.1172.0",
    "bats": "^1.6.0",
    "chalk": "^2.4.2",
    "cheerio": "^1.0.0-rc.10",
    "ci-info": "^2.0.0",
    "cli-highlight": "^2.1.11",
    "cli-table3": "^0.6.2",
    "@artilleryio/int-commons": "*",
    "@artilleryio/int-core": "*",
    "csv-parse": "^4.16.3",
    "debug": "^4.3.1",
    "dotenv": "^16.0.1",
    "eventemitter3": "^4.0.4",
    "fs-extra": "^10.1.0",
    "ip": "^1.1.8",
    "jmespath": "^0.16.0",
    "joi": "^17.6.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.19",
    "mime-types": "^2.1.35",
    "moment": "^2.29.4",
    "ora": "^4.0.4",
    "posthog-node": "^1.3.0",
    "sprintf-js": "^1.1.2",
    "sqs-consumer": "^5.7.0",
    "temp": "^0.9.4",
    "tmp": "0.2.1",
    "try-require": "1.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^7.1.2",
    "@hapi/hapi": "^20.1.3",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "execa": "^0.10.0",
    "get-bin-path": "^5.1.0",
    "husky": "^4.2.5",
    "prettier": "^2.5.1",
    "rewiremock": "^3.14.3",
    "sinon": "^4.5.0",
    "tap": "^15.2.3",
    "zx": "^4.2.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
